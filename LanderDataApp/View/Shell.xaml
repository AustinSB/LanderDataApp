        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LanderDataApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        xmlns:vm="clr-namespace:LanderDataApp.ViewModel">
    <Window.DataContext>
        <vm:ShellViewModel />
    </Window.DataContext>

    <Grid>
        <Label x:Name="HelloLabel" Content="{Binding HelloString}"/>
